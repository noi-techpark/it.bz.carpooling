@media (max-width: 768px) {
  .no-mobile {
    display: none; } }

input {
  height: 60px;
  font-size: 1.1rem;
  background: white;
  color: black;
  border: none;
  border-bottom: 1px solid #7e7e7e; }
  input::placeholder {
